    "lint-staged": "^13.2.1",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.6",
    "webpack": "^5.79.0"
  },
  "lint-staged": {
    "*.{js,jsx,.json}": [
      "npm run lint",
      "npm run format"
    ],
    "*.--write": "npm run format"
  }
